{
  "secureHeaders": true,
  "contentSecurityPolicy": "default-src 'self'; script-src 'self'",
  "xssProtection": true,
  "rateLimit": {
    "windowMs": 60000,
    "max": 100
  },
  "noSQLInjectionSanitize": true,
  "sqlInjectionSanitize": true,
  "httpParameterPollutionPrevention": true,
  "cors": {
    "enabled": true,
    "origin": "https://example.com"
  }
}